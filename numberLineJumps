// from Hacker Rank - Number Line Jumps

 x1 = 14 // starting position for kangaroo 1
 v1 = 4 // jump distance for kangaroo 1
 x2 = 98 // starting position for kangaroo 2
 v2 = 2 // jump distance for kangaroo 2

 function kangaroo(x1,v1,x2,v2) {
	for (let i = 0; i <= 10000; i++) {
		x1 += v1
		x2 += v2
		if (x1 === x2) {
			return 'YES'
		} 
    }
	return 'NO'
 }
 console.log(kangaroo(x1,v1,x2,v2))

