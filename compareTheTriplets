// from HackerRank CompareTheTriplets

function compareTriplets(a, b) {
    let aliceScore = 0;
    let bobScore = 0;
    
    for (let i = 0; i < b.length; i++) {

        if (a[i] > b[i]) {
           aliceScore = aliceScore + 1
        } 
        
        if(b[i] > a[i]){
          bobScore = bobScore + 1
        }                
    }
        return [aliceScore, bobScore]
}
